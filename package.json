{
  "name": "gras",
  "version": "1.0.1",
  "description": "Generate random albums",
  "main": "lib/main.js",
  "scripts": {
    "build": "gulp build",
    "example": "./script/example | imgcat",
    "test": "xo && ava"
  },
  "author": "Karl Brightman <karl@karlbright.org>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-newer": "^1.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.7",
    "through2": "^2.0.1",
    "xo": "^0.16.0"
  },
  "dependencies": {
    "async": "^2.0.1",
    "canvas": "^1.5.0",
    "cheerio": "^0.22.0",
    "got": "^6.5.0",
    "inflection": "^1.10.0",
    "intrinsic-scale": "^3.0.3",
    "smartcrop": "^1.1.1",
    "smartcrop-gm": "^1.0.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports"
    ]
  },
  "xo": {
    "ignores": [
      "lib/**/*"
    ],
    "semicolon": false,
    "space": 2
  }
